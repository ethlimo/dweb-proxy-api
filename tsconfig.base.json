{
    "compilerOptions": {
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "moduleResolution": "NodeNext",
      "noEmitOnError": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "preserveSymlinks": true,
      "preserveWatchOutput": true,
      "pretty": false,
      "rootDir": "./src",
      "strict": true,
      "sourceMap": true,
      "target": "es2022",
      "module": "NodeNext"
    },
  }
  