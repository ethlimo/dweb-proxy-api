{
  "name": "dweb-api-server",
  "version": "1.0.0",
  "description": "Proxy middleware for ENS and other on chain naming services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon -e js,ts --watch src --exec \"npx tsx src/index.ts\"",
    "fmt": "prettier src -w",
    "test": "mocha --exit",
    "test:coverage": "nyc mocha --exit",
    "build": "tsc",
